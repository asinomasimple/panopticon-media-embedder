{
  "manifest_version": 3,
  "name": "Panopticon Media Embedder",
  "version": "0.1",
  "description": "Enhance QBN posts by embedding Instagram, webp images, and more.",
  "permissions": [
    "scripting",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "*://*.qbn.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.qbn.com/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "options_page": "options.html",
  "icons": {
    "16": "icons/icon32.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}